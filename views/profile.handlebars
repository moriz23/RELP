{{#each user}}
  <div class="row">
    <div class="col s4">
      <h2>{{this.dataValues.firstname}} {{this.dataValues.lastname}}</h2>
      <p>Created Accout: {{this.createdAt}}</p>
    </div>
    <div class="col s4" id="interest">
      <p class="pull right">About</p>
      <div class="divider"></div>
      <form action="/profile/:user" method="POST">
        <div class="input-field col s6 m4 black-text">
          <input type="text" name="birthday" class="validate" required="true">
          <label for="firstname">Birthday</label>
        </div>
      </form>
    </div>
  </div>
{{/each}}
